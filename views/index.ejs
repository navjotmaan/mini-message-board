<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <a href="new">New Message</a>

        <h2>All Messsages</h2>
        <% messages.forEach(message => { %>
            <div>
                <%= message.text %>
                <%= message.user %>
                <%= message.added %>
            </div>
        <% }); %>

    </body>
</html>